{
  "name": "serrapi",
  "version": "1.0.0",
  "main": "build/app.js",
  "license": "MIT",
  "babel": {
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ],
    "presets": [
      "babel-preset-env"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production babel ./src -d build",
    "start": "NODE_ENV=production node build/app",
    "dev": "nodemon ./src/app.js --exec babel-node"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "express": "^4.16.2",
    "leven": "^2.1.0",
    "lodash": "^4.17.4",
    "node-cron": "^1.2.1",
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.14.11"
  }
}
