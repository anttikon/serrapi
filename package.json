{
  "name": "serrapi",
  "version": "1.0.0",
  "main": "build/app.js",
  "license": "MIT",
  "babel": {
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ],
    "presets": [
      "babel-preset-env"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production babel ./src -d build",
    "start": "NODE_ENV=production node build/app",
    "test": "npm run init-dev && JSON_FILE=local jest",
    "dev": "npm run init-dev && JSON_FILE=local nodemon ./src/app.js --exec babel-node",
    "init-dev": "[ -f ./AllSets.json ] || curl -O https://mtgjson.com/json/AllSets.json"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "leven": "^2.1.0",
    "lodash": "^4.17.4",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^23.6.0",
    "nodemon": "^1.14.11",
    "supertest": "^3.0.0"
  }
}
